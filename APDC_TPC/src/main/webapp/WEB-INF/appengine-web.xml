<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <threadsafe>true</threadsafe>
  <sessions-enabled>false</sessions-enabled>
  <runtime>java8</runtime>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>

<static-files>
<include path="/**.html" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.css" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.js" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.jpg" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.gif" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.png" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.mp3" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.wav" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.ttf" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.ico" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
<include path="/**.cur" expiration="0m" /> <!-- 1 minute, normal is 12h or more -->
</static-files>
</appengine-web-app>